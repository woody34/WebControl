<Window x:Class="WebControl.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WebBrowser" Height="300" Width="1200">
    <Window.CommandBindings>
        <CommandBinding Command="NavigationCommands.BrowseBack" CanExecute="BrowseBack_CanExecute" Executed="BrowseBack_Executed" />
        <CommandBinding Command="NavigationCommands.BrowseForward" CanExecute="BrowseForward_CanExecute" Executed="BrowseForward_Executed" />
        <CommandBinding Command="NavigationCommands.GoToPage" CanExecute="GoToPage_CanExecute" Executed="GoToPage_Executed" />
    </Window.CommandBindings>
    <DockPanel>
        <ToolBar DockPanel.Dock="Top">
            <Button Command="NavigationCommands.BrowseBack" Content="B" />
            <Button Command="NavigationCommands.BrowseForward" Content="F" />
            <Separator />
            <TextBox Name="txtUrl" Width="300" KeyUp="txtUrl_KeyUp" />
            <Button Command="NavigationCommands.GoToPage" Content="Go"/>
        </ToolBar>
        <ToolBar DockPanel.Dock="Bottom">
            <Label Content="IP List(CSV):" />
            <Button Name="btnIP" Content="Browse" Click="btnIP_Click" />
            <Label Content="@remote Codes(CSV):" />
            <Button Name ="btnRemoteCodes" Content="Browse" Click="btnRemoteCodes_Click"/>
            <CheckBox Name="ckboxActivateRemote">@remote</CheckBox>
            <CheckBox Name="ckboxCounter">Conter</CheckBox>
            <Button Name="btnStart" Content="Start" Click="btnStart_Click" />
        </ToolBar>
        <WebBrowser Name="webBrowser" Navigating="webBrowser_Navigating"  />
    </DockPanel>
</Window>